{
  "name": "frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "lint": "eslint .",
    "typecheck": "react-router typegen && tsc",
    "docker:build": "docker build -t frontend .",
    "docker:rebuild": "docker stop frontend 2>nul || echo Container not running && docker rm frontend 2>nul || echo Container not found && docker build -t frontend .",
    "docker:run": "docker run -d -p 3000:3000 --env-file .env --name frontend frontend",
    "docker:stop": "docker stop frontend",
    "docker:remove": "docker rm frontend"
  },
  "dependencies": {
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "@tanstack/react-query": "^5.84.2",
    "@tanstack/react-query-devtools": "^5.85.0",
    "@types/axios": "^0.9.36",
    "@types/react-router": "^5.1.20",
    "axios": "^1.11.0",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "^9.32.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
